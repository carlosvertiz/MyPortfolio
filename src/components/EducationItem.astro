---
interface Props {
	title: string;
  place: string;
  dateInt: string;
  dateEnd: string;
}

const { title, place, dateInt, dateEnd } = Astro.props;
---


<article class="w-full pl-5 py-2 pr-10 rounded-3xl">
  <h3>{title}</h3>
  <p>{place} | {dateInt} {dateEnd? `- ${dateEnd}` : ""}</p>
</article>