---
import AstroIcon from "../svg/AstroIcon.astro";
import CSSIcon from "../svg/CSSIcon.astro";
import HTMLIcon from "../svg/HTMLIcon.astro";
import JavaScriptIcon from "../svg/JavaScriptIcon.astro";
import ReactIcon from "../svg/ReactIcon.astro";
import TailWindIcon from "../svg/TailWindIcon.astro";
---
<footer class="flex flex-row justify-end gap-5 pr-10  w-screen
              border.solid border border-indigo-800/80 border-b-0
              h-fit
              rounded-t-2xl
              bg-[#000000] bg-[radial-gradient(#ffffff33_1px,#00091d_1px)]
              mt-10 py-3 px-5">
              <AstroIcon class="size-7"/>
              <HTMLIcon class="size-7"/>
              <CSSIcon class="size-7"/>
              <TailWindIcon class="size-7"/>
              <ReactIcon class="size-7" />
              <JavaScriptIcon class="size-7"/>
</footer>