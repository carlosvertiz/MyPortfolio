---
import Layout from '../../layouts/Layout.astro'
import AboutMe from '../../components/AboutMe.astro'
import Experience from '../../components/Experience.astro'
import Education from '../../components/Education.astro'
import Projects from '../../components/Projects.astro'
import Skills from '../../components/Skills.astro'
import {data, sections} from '../../Languages/DataEs.js'
import {ChangeLanguages} from '../../components/ChangeLanguages.jsx'
---

<Layout sections = {sections} title="Portfolio Carlos Vertiz, developer and programmer">
	<main class="flex flex-col max-w-7xl">
		<ChangeLanguages client:only/>
		<AboutMe  data={data} id="home" />
		<Experience section = {sections.Experience} data={data} id="experience" />
		<Education section = {sections.Education} data={data} id="education" />
		<Skills section = {sections.Skills} data={data} id="skills" />
		<Projects section = {sections.Projects} data={data} id="projects" />
	</main>
</Layout>